pipeline {
   agent any
   tools {
        maven 'Maven'

    }
   stages {
        stage('Build') {
            steps {
               snDevOpsStep ()
                echo "publish in progress"   
            }
        }
        stage('Test') { 
            steps {
               snDevOpsStep ()
                echo "publish in progress"   
            }
         
        }
    
       
    
      stage('Publish') {
             steps{
                  snDevOpsStep ()
                  sleep 30
                  echo "publish in progress"     
              }
      }
      
       stage('Deploy') {
             steps{
                  snDevOpsStep ()
                  snDevOpsChange()
                  echo 'deployment started'      
              }
      }
      
  }
}
